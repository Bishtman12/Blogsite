<%- include("partials/header") %>
    <h1>Home</h1>
    <%postList.forEach(function(post){ %>
        <div class="PostContainer">
         <h2><%=post.title%></h2>
         <p ><%=post.article.slice(0,120) + "..." %>
            <a href="/post/<%=post._id%>">Read More</a>
        </p>
        </div>
    <%});%>
<%- include("partials/footer") %>
